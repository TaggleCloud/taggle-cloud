%p#notice= notice
.row-fluid
  .span9
    %h1= @conference.name
    %h3= @conference.location
  .span3
    = link_to 'Edit', edit_conference_path(@conference)
    \|
    = link_to 'Back', conferences_path

%h2= link_to "Attendees", conference_attendees_path(@conference)
%hr
%ul.row.attendees
  - @attendees.each do |attendee|
    %li.three.columns
      - if attendee.user_id?
        = link_to "User Name", user_path(attendee.user_id)
      = attendee.registered_email
